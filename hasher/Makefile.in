	$(E_rm)rm -f$(V) *.set.txt $(CONFIGURE_SH)

$(Clip_B_Dir)/clip_hashextract$(Suff_Exe) : Makefile
	$(E_cd)cd $(Clip_M_Dir)/clip/clip_hashextract \
		&& ( ( [ -f Makefile ] && [ -f $(CONFIGURE) ] ) || $(CONFIGURE_DIR) ) \
		&& $(MAKE) $(MAKEFLAGS) install $(ADDMAKECMDGOALS)

configuration :
	$(E_if)[ -d ../temp ] 	|| mkdir ../temp
	$(E_if)[ -d hashed ] 	|| mkdir hashed

distclean : $(DIRS_TO_SCAN) clean
	$(E_rm)rm -f$(V) Makefile.ini

distcleanc :

hash : Makefile $(Clip_I_Dir)/hashcode.h

hash1 : Makefile

$(Clip_I_Dir)/hashcode.h : $(Clip_B_Dir)/clip_hashextract$(Suff_Exe) $(wildcard hashed/* ) Makefile
	$(Trace_o)
	$(E_cd)cd hashed \
		&& $(Clip_B_Dir)/clip_hashextract$(Suff_Exe) * >../hashcode.h
	$(E_xx)sort <hashcode.h >../temp/hash.tmp
	@sleep 1
	$(E_xx)sort -u <../temp/hash.tmp -o hashcode.h
	@sleep 1
	$(E_rm)rm -f$(V) $(Clip_I_Dir)/hashcode.h
	$(E_if)[ -d $(Clip_I_Dir) ] || mkdir -p$(V) $(Clip_I_Dir)
	$(E_e)echo "#ifndef HASH_args" 	> $(Clip_I_Dir)/hashcode.h
	$(E_xx)cat $(PWD)/hashcode.h >>$(Clip_I_Dir)/hashcode.h
	$(E_e)echo "#endif" >>$(Clip_I_Dir)/hashcode.h

