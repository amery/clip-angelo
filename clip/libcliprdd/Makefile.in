#
#                                            clip/libcliprdd/Makefile.in
#
Local_Depend 	= 		../include/*.ch 		\
							../include.h/*.h

c_files 			= 		$(sort 						\
										rddclip.c 		\
										rdd.c 			\
										dbf.c 			\
										ntx.c 			\
										cdx.c 			\
										dbt.c 			\
										fpt.c 			\
										rushmore.c 		\
										btree.c 			\
										six.c 			\
										hiper.c 			\
							)

PROG 				= 		cliprdd
ADDLIBS 			=
o_files 				= 			$(addsuffix .o, $(basename $(prg_files) $(c_files) ) )
SRC_C_files 		= 			$(sort $(c_files) )
SRC_PRG_files 		= 			$(sort $(prg_files) )

export

######################################################################################
###################################################################################### local rules

install all: Makefile $(o_files) $(ATARGET)

hash1 :
	$(E_xx)$(Clip_M_Dir)/init/LinkAllFiles.sh clip-libcliprdd

cfg :
	$(E_cp)cp --remove-destination -fu$(V) *.h $(Clip_I_Dir)/

cleanc:
	$(E_rm)rm -f$(V) 	*.b 			\
							*.tmp 		\
							test_ref.c 	\
							test.c 		\
							test.log 	\
							*.dbf 		\
							*. 			\
							.ntx 			\
							*.cdx 		\
							*.dbt 		\
							*.fpt 		\
							$(ATARGET) 	\
							test

distcleanc :
	$(E_rm)rm -rf$(V) *.g

rddclip.o:	rddclip.c ../include.h/ci_rdd.h Makefile
	$(E_cc)if [ -f $(subst .o,.g,$@) ] ; then 	\
		$(Trace_cc_d) ; 									\
		$(CC) $(C_FLAGS) -g -c $< ; 					\
	else 														\
		$(Trace_cc) ; 										\
		$(CC) $(C_FLAGS) -c $< ; 						\
	fi

