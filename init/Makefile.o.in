.prg.po : Makefile $(CLIP)
	$(Trace_prg)
	$(E_clip)$(CLIP) $(CLIPFLAGS) -p $<

.prg.o : Makefile $(CLIP)
	$(Trace_prg)
	$(E_clip)$(CLIP) $(CLIPFLAGS) $<

