%.o : %.c Makefile
	$(E_cc)if [ -f $(subst .o,.g,$@) ] ; then 		\
		$(Trace_cc_d) ; 										\
		$(CC) -g $(C_FLAGS) -c $< ; 						\
	else 															\
		$(Trace_cc) ; 											\
		$(CC) $(C_FLAGS) -c $< ; 							\
	fi

.c.o :  Makefile
	$(E_cc)if [ -f $(subst .o,.g,$@) ] ; then 		\
		$(Trace_cc_d) ; 										\
		$(CC) -g $(C_FLAGS) -c $< ; 						\
	else 															\
		$(Trace_cc) ; 											\
		$(CC) $(C_FLAGS) -c $< ; 							\
	fi

%.o : %.prg Makefile $(CLIP)
	$(E_clip)if [ -f $(subst .o,.g,$@) ] ; then 		\
		$(Trace_prg_d) ; 										\
		$(CLIP) -g $(CLIPFLAGS) $< ; 						\
	else 															\
		$(CLIP) $(CLIPFLAGS) $< ; 							\
		$(Trace_prg) ; 										\
		$(CLIP) $(CLIPFLAGS) $< ;							\
	fi

.prg.o : Makefile $(CLIP)
	$(E_clip)if [ -f $(subst .o,.g,$@) ] ; then 		\
		$(Trace_prg_d) ; 										\
		$(CLIP) -g $(CLIPFLAGS) $< ; 						\
	else 															\
		$(CLIP) $(CLIPFLAGS) $< ; 							\
		$(Trace_prg) ; 										\
		$(CLIP) $(CLIPFLAGS) $< ;							\
	fi

%.po : %.prg Makefile $(CLIP)
	$(E_clip)if [ -f $(subst .po,.g,$@) ] ; then 	\
		$(Trace_prg_d) ; 										\
		$(CLIP) -g -p $(CLIPFLAGS) $< ; 					\
	else 															\
		$(Trace_prg) ; 										\
		$(CLIP) -p $(CLIPFLAGS) $< ; 						\
	fi

.prg.po : Makefile $(CLIP)
	$(E_clip)if [ -f $(subst .po,.g,$@) ] ; then 	\
		$(Trace_prg_d) ; 										\
		$(CLIP) -g -p $(CLIPFLAGS) $< ; 					\
	else 															\
		$(Trace_prg) ; 										\
		$(CLIP) -p $(CLIPFLAGS) $< ; 						\
	fi


