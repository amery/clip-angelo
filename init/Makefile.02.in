endef

define Trace_n
	echo [Making] : $(value PWD)/$@ because of : $?
endef

define Trace_c
	$(Trace_n) 								; \
	echo [CC] $(value PWD)/$@ 	>&0 	; \
	echo [CC] $(C_FLAGS) $(value PWD)/$@
endef

define Trace_c_d
	$(Trace_n) 									; \
	echo [CC -g] $(value PWD)/$@ 	>&0 	; \
