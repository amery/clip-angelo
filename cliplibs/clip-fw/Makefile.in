#
#                                            clip-fw/Makefile.in
#
Local_Depend = include/*.ch include/*.h

CLIPFLAGS += $(INCLUDEDIRS)
PROG = clip-fw
CLIPLIBS  =
COBJS    = c_funcs.o
LIBOBJS  = 	fw_sysgtk.o fw_common.o fw_syscti.o fw_menusys.o 								\
	   		fw_color.o fw_window.o fw_menu.o fw_menuitem.o 									\
	   		fw_msg.o fw_font.o fw_msgbar.o fw_bar.o fw_btnbmp.o 							\
	   		fw_control.o fw_dialog.o fw_combobox.o fw_get.o fw_say.o fw_button.o 	\
	   		fw_meter.o fw_listbox.o fw_msgitem.o fw_timer.o fw_rect.o					\
	   		fw_mdiclien.o fw_mdichild.o fw_mdiframe.o fw_scrllbar.o fw_tabs.o 		\
	   		fw_wbrowse.o

OBJS = $(sort $(COBJS) $(LIBOBJS) )
.SUFFIXES: .prg .o .po

################################################################################################################

all:  Makefile links  $(STARGET)

cfg :
	$(E_mkD)mkdir -p $(Clip_I_Dir)/$(PROG)
	$(E_cp)cp --remove-destination -fpu$(V) ./include/*.ch $(Clip_I_Dir)/$(PROG)/
	$(E_cp)cp --remove-destination -fpu$(V) ./include/*.h $(Clip_I_Dir)/$(PROG)/

links :

cleanc:
	$(E_rm)rm -f$(V) fw*.c $(TARGET).* Makefile.inc

distcleanc :
	$(E_rm)rm -rf$(V) *.g

install: all install-lib install-example

include $(Makefile_o_in)

include $(Makefile_end_in)

