Depend_on 				= 			$(Clip_L_Dir)/libclip-fw.so 		\
										$(Clip_L_Dir)/libclip-gtk.so 		\
										$(Clip_L_Dir)/libclip-cti.so
CLIPFLAGS 				= 			-o$@ -g -l -w $(INCLUDEDIRS)
CLIPLIBS  				=
OBJS  					=
BINS 						= 			$(sort 								\
													combos$(EXESUFF) 		\
													dlgtest$(EXESUFF) 	\
													gettest$(EXESUFF) 	\
													lines$(EXESUFF) 		\
													test$(EXESUFF) 		\
													testbar$(EXESUFF) 	\
													testbrw$(EXESUFF) 	\
													testlbx$(EXESUFF) 	\
													testscrl$(EXESUFF) 	\
													testwsty$(EXESUFF) 	\
													tooltip$(EXESUFF) 	\
													atest$(EXESUFF) 		\
													bmp$(EXESUFF)			\
										)

#atest$(EXESUFF)  # 		missing many functions
#bmp$(EXESUFF)  	# 		missing many functions
LIBS 						+= 		-lclip-fw -lclip-gtk -lclip-cti $(PIXBUFLIBS)
export

