Local_Depend =
#$(Clip_L_Dir)/libclip-gtkextra.so $(Clip_L_Dir)/libclip-gtk.so


################################################################################################################

all:Makefile sheet$(EXESUFF)

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b sheet$(EXESUFF)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

sheet$(EXESUFF): sheet.prg $(CLIP) $(LIBCLIP)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -e -s -l sheet.prg -lclip-gtkextra -lclip-gtk

include $(Makefile_o_in)

include $(Makefile_end_in)

