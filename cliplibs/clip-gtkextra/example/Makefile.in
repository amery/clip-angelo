Depend_on 			= 			$(Clip_L_Dir)/libclip-gtkextra.so 	\
									$(Clip_L_Dir)/libclip-gtk.so

export

######################################################################################
###################################################################################### local rules

all:Makefile #sheet$(EXESUFF) ## missing functions

install : all

hash1 :

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b sheet$(EXESUFF)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

sheet$(EXESUFF): sheet.prg $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -ew -s -l sheet.prg -lclip-gtkextra -lclip-gtk $(LIBS)

include $(Makefile_o_in)

include $(Makefile_end_in)

