Depend_on 			= 			$(Clip_L_Dir)/libclip-oracle.so
PRG 					= 			my_sqlplus$(EXESUFF)
LIBS 					= 			-L$(OR_LIB) -lclntsh -lnnz11
# -laio

export

######################################################################################
###################################################################################### local rules

all:Makefile  $(PRG)

install : all

hash1 :

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b $(PRG)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

my_sqlplus$(EXESUFF) 	: my_sqlplus.prg $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -ewMs my_sqlplus.prg -lclip-oracle $(LIBS)

include $(Makefile_o_in)

include $(Makefile_end_in)

