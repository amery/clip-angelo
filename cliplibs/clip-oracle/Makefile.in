#
#                                            clip-oracle/Makefile.in
#
#  GNU make makefile
#
Local_Depend = include/*.ch

PROG = clip-oracle
OBJS=or_clip.o or.o
#ADDLIBS = -L$(OR_LIB) -lclntsh
C_FLAGS += -I$(OR_INC)
ADDLIBS =
ADDLIBS2 =

DOC = *.html

################################################################################################################

all: Makefile  have_lib $(OBJS) $(STARGET)

cfg :
	$(E_cp)cp --remove-destination -fpu$(V) include/oracle.ch $(Clip_I_Dir)/

have_lib:
	$(E_if)test -f $(OR_INC)/oci.h

include $(Makefile_o_in)

install : all install-lib install-example install-doc

cleanc:
	$(E_rm)rm -rf$(V) or.c $(TARGET).*

distcleanc :
	$(E_rm)rm -rf$(V) *.g

include $(Makefile_end_in)

