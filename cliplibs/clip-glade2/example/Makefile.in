#
#                                            clip-glade2/example/Makefile.in
#
# Make for eglade
Local_Depend =
Depend_on = $(Clip_L_Dir)/libclip-glade2.so $(Clip_L_Dir)/libclip-gtk2.so

#PRGFLAGS=-w -l

PRGLIBS=-lclip-gtk2 -lclip-glade2

OBJS = $(sort dlgopen.o dlgsave.o dlgselfont.o dlgabout.o )

.SUFFIXES: .prg .o

include Makefile.01

all: Makefile eglade$(EXESUFF)

install : all

cfg :

Makefile.01 :
	cp -f$(V) ../Makefile.01 .
eglade$(EXESUFF): eglade.prg  $(OBJS) $(CLIP) $(LIBCLIP)
	$(Trace_clip)
	$(CLIP) $(CLIPFLAGS) -o$@ -ewlNs $<  $(OBJS) $(PRGLIBS) $(LIBS)

cleanc:
	$(E_rm)rm -f$(V) eglade$(EXESUFF) *.c

distcleanc :
	$(E_rm)rm -rf$(V) *.g

include $(Makefile_o_in)

include $(Makefile_end_in)

