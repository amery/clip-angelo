#
#                                            clip-glade2/example/Makefile.in
#
# Make for eglade
Depend_on 					= 			$(Clip_L_Dir)/libclip-glade2.so 		\
											$(Clip_L_Dir)/libclip-gtk2.so
LIBS 							+= 		-lclip-gtk2 -lclip-glade2
OBJS 							= 			$(sort 					\
													dlgopen.o 		\
													dlgsave.o 		\
													dlgselfont.o 	\
													dlgabout.o 		\
											)

export

######################################################################################
###################################################################################### local rules

all: Makefile $(OBJS) eglade$(EXESUFF)

install : all

hash1 :

cfg :

eglade$(EXESUFF): eglade.prg  $(OBJS) $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) $(CLIPFLAGS) -o$@ -ewlNs $<  $(OBJS) $(LIBS)

cleanc:
	$(E_rm)rm -f$(V) eglade$(EXESUFF) *.c

distcleanc :
	$(E_rm)rm -rf$(V) *.g

include $(Makefile_o_in)

include $(Makefile_end_in)

