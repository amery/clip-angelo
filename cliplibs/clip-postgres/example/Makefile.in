Local_Depend =
#$(Clip_L_Dir)/libclip-postgres.so


PRG=my_psql$(EXESUFF)

################################################################################################################

all: Makefile $(PRG)

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b $(PRG)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

my_psql$(EXESUFF): my_psql.prg $(CLIP) $(LIBCLIP)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -eMs my_psql.prg -lclip-postgres

include $(Makefile_o_in)

include $(Makefile_end_in)

