#
#                                            clip-postgres/example/Makefile.in
#
Depend_on 			= 			$(Clip_L_Dir)/libclip-postgres.so
LIBS 					= 			-L$(PGLIBDIR) -lpq
ADDLIBS 				=
PRG 					= 			my_psql$(EXESUFF)

export

######################################################################################
###################################################################################### local rules

all: Makefile $(PRG)

install : all

hash1 :

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b $(PRG)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

my_psql$(EXESUFF): my_psql.prg $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -ewMs my_psql.prg -lclip-postgres $(LIBS)

include $(Makefile_o_in)

include $(Makefile_end_in)

