Depend_on 				= 				$(Clip_L_Dir)/libclip-interbase.so
PRG 						= 				my_isql$(EXESUFF)

export

######################################################################################
###################################################################################### local rules

all:Makefile $(PRG)

install : all

hash1 :

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b $(PRG)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

my_isql$(EXESUFF): my_isql.prg $(CLIP) $(LIBCLIP) $(Depend_on)
	$(Trace_clip)
	$(CLIP) -o$@ -ewMs my_isql.prg -lclip-interbase

include $(Makefile_o_in)

include $(Makefile_end_in)

