Local_Depend =
#$(Clip_L_Dir)/libclip-interbase.so

PRG=my_isql$(EXESUFF)

################################################################################################################

all:Makefile $(PRG)

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b $(PRG)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

my_isql$(EXESUFF): my_isql.prg $(CLIP) $(LIBCLIP)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -eMs my_isql.prg -lclip-interbase

include $(Makefile_o_in)

include $(Makefile_end_in)

