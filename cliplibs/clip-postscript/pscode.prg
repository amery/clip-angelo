#include "ci_clip-postscript.ch"

/* TODO: Class for Postscript code part */

function PSCode( code )
	local obj := map()

	obj:className := "PSCode"
	obj:code := code

	_recover_PSCODE(obj)
return obj


function _recover_PSCODE( obj )
return obj