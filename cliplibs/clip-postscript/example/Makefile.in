Local_Depend =
# $(Clip_L_Dir)/libclip-postscript.so

.SUFFIXES: .prg .o .po
.PHONY: all clean

# Form test
PRG=testform$(EXESUFF)
LIBS=-lclip-postscript -lclip-xml -lexpat

################################################################################################################

all: Makefile $(PRG)

cfg :

cleanc:
	$(E_rm)rm -f$(V) $(PRG) *.c

distcleanc :
	$(E_rm)rm -rf$(V) *.g

$(PRG): testform.prg $(CLIP) $(LIBCLIP)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -ewsln $(CLIPINCLUDE) testform.prg $(LIBS)

include $(Makefile_o_in)

include $(Makefile_end_in)

