Depend_on 				= 				$(Clip_L_Dir)/libclip-postscript.so $(Clip_L_Dir)/libclip-xml.so
CLIPFLAGS 				+= 			-I../include
PRG 						= 				testform$(EXESUFF)
LIBS 						= 				-lclip-postscript -lclip-xml -lexpat -lps
ADDLIBS 					=

export

######################################################################################
###################################################################################### local rules

all: Makefile $(PRG)

install : all

hash1 :

cfg :

cleanc:
	$(E_rm)rm -f$(V) $(PRG) *.c

distcleanc :
	$(E_rm)rm -rf$(V) *.g

$(PRG): testform.prg $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -ewsNln $(CLIPFLAGS) testform.prg $(LIBS)

include $(Makefile_o_in)

include $(Makefile_end_in)

