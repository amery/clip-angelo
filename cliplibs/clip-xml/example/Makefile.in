Local_Depend =
# $(Clip_L_Dir)/libclip-xml.so

.SUFFIXES: .prg .o .po
.PHONY: all clean

# Widget tests
ADDLIBS=-lclip-xml

PROGS=test_xml$(EXESUFF) test$(EXESUFF)

EXTRALIBS=

all: Makefile $(PROGS)

install : all

cfg :

cleanc:
	$(E_rm)rm -f$(V) $(PROGS)
	$(E_rm)rm -f$(V) *.c *.b

distcleanc :
	$(E_rm)rm -rf$(V) *.g

test_xml$(EXESUFF): test_xml.prg $(CLIP) $(LIBCLIP)
	$(Trace_clip)
	$(CLIP) -o$@ -ewsln  $(C_FLAGS) test_xml.prg $(ADDLIBS) $(EXTRALIBS)

test$(EXESUFF): test.prg $(CLIP) $(LIBCLIP)
	$(Trace_clip)
	$(CLIP) -o$@ -eswlg test.prg $(ADDLIBS)

include $(Makefile_o_in)

include $(Makefile_end_in)

