#
Local_Depend =
#$(Clip_L_Dir)/libclip-bz2.so


PRG=bz2$(EXESUFF)
################################################################################################################

all: Makefile $(PRG)

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b $(PRG) *.bz2

distcleanc :
	$(E_rm)rm -rf$(V) *.g

bz2$(EXESUFF): bz2.prg $(CLIP) $(LIBCLIP)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -e -s -l -M bz2.prg -lclip-bzip2

include $(Makefile_o_in)

include $(Makefile_end_in)

