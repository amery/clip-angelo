#
#                                            clip-bz2/example/Makefile.in
#
Depend_on 			= 			$(Clip_L_Dir)/libclip-bzip2.so
PRG 					= 			bz2$(EXESUFF)
LIBS 					= 			-lclip-bzip2 -lbz2
export

######################################################################################
###################################################################################### local rules

all: Makefile $(PRG)

install : all

hash1 :

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b $(PRG) *.bz2

distcleanc :
	$(E_rm)rm -rf$(V) *.g

bz2$(EXESUFF): bz2.prg Makefile $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -ewN -s -l -M bz2.prg $(LIBS)

include $(Makefile_o_in)

include $(Makefile_end_in)

