Depend_on 			=		 $(Clip_L_Dir)/libclip-gd.so
prg_files 			= 		gr.prg
SRC_C_files 		= 		$(sort $(c_files) )
SRC_PRG_files 		= 		$(sort $(prg_files) )
exe_files 			= 		$(addsuffix $(EXESUFF), $(basename $(prg_files) ) )
export

######################################################################################
###################################################################################### local rules

all: Makefile $(exe_files)

install : all

hash1 :

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b $(exe_files)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

gr$(EXESUFF): gr.prg $(CLIP) $(FULL_LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -swlN -e -M gr.prg -lclip-gd $(LIBS)
# -lgd

include $(Makefile_o_in)

include $(Makefile_end_in)

