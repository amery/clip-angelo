PROG 					= 			gd

SRCS 					= 								\
									gd.c 				\
									gdcache.c 		\
									gdfontg.c 		\
									gdfontl.c 		\
									gdfontmb.c 		\
									gdfonts.c 		\
									gdfontt.c 		\
									gdft.c 			\
									gd_gd2.c 		\
									gd_gd.c 			\
									gdhelpers.c 	\
									gd_io.c 			\
									gd_io_dp.c 		\
									gd_io_file.c 	\
									gd_io_ss.c 		\
									gd_jpeg.c 		\
									gdkanji.c 		\
									gd_png.c 		\
									gdsector.c 		\
									gd_ss.c 			\
									gdtables.c 		\
									gdttf.c 			\
									gd_wbmp.c 		\
									gdxpm.c 			\
									wbmp.c

OBJS 					= 			$(sort 	$(addsuffix .o, $(basename $(SRCS) ) ) )
INCDIR 				= 			-I ../include $(INCLUDEDIRS)
C_FLAGS 				+= 		$(INCDIR)

export

######################################################################################
###################################################################################### local rules

all : Makefile cfg $(OBJS) $(ATARGET)

install : all

Makefile.inc :
	$(E_xx)$(CONFIGURE_SH)

hash1 :
	$(E_xx)$(Clip_M_Dir)/init/LinkAllFiles.sh clip-gd-libgd

cfg :

links :

cleanc:
	$(E_rm)rm -rf$(V) $(TARGET).*

distcleanc :
	$(E_rm)rm -rf$(V) *.g

include $(Makefile_o_in)

include $(Makefile_end_in)

