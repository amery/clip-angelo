CLIP_MODULE = clip-ui
Local_Depend =
CLIPFLAGS += -I../../clip-gtk2 -n
OBJS    = driver-gtk2.po

.PHONY: all clean uninstall distclean

################################################################################################################

all : Makefile $(OBJS)

cfg :

cleanc:

distcleanc :
	$(E_rm)rm -rf$(V) *.g

install: all
	$(E_mkD)mkdir -p$(V) $(INSTDIR)/lib/drivers
	$(E_cp)$(CLIP_CP) $(OBJS) $(INSTDIR)/lib/drivers/

uninstall:
	$(E_rm)rm -f$(V) $(Clip_L_Dir)/drivers/$(OBJS)

include $(Makefile_o_in)

include $(Makefile_end_in)

