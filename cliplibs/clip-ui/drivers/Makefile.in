CLIP_MODULE 			= 			clip-ui
Local_Depend 			=
CLIPFLAGS 				+= 		-I../../clip-gtk2/include -n
OBJS 						= 			driver-gtk2.po

export

######################################################################################
###################################################################################### local rules

all : Makefile $(OBJS)

install : all

hash1 :
	$(E_xx)$(Clip_M_Dir)/init/LinkAllFiles.sh clip-ui-drivers

cfg :

cleanc:

distcleanc :
	$(E_rm)rm -rf$(V) *.g

install: all
	$(E_mkD)mkdir -p$(V) $(INSTDIR)/lib/drivers
	$(CLIP_CP) $(OBJS) $(INSTDIR)/lib/drivers

uninstall:
	$(E_rm)rm -f$(V) $(Clip_L_Dir)/drivers/$(OBJS)

include $(Makefile_o_in)

