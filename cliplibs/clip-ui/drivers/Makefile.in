CLIP_MODULE = clip-ui
Local_Depend =
CLIPFLAGS += -I../../clip-gtk2 -n
OBJS    = driver-gtk2.po

.PHONY: all clean uninstall distclean

all : Makefile $(OBJS)

cfg :

cleanc:

distcleanc :
	rm -rf$(V) *.g

install: all
	mkdir -p$(V) $(DESTDIR)/lib/drivers
	$(CLIP_CP) $(OBJS) $(DESTDIR)/lib/drivers

uninstall:
	rm -f$(V) $(Clip_L_Dir)/drivers/$(OBJS)

include $(Makefile_o_in)

#driver-gtk2.po: driver-gtk2.prg $(CLIP)
#	$(Trace_clip)
#	$(CLIP) $(CLIPFLAGS) -I../../clip-gtk2 -n -p driver-gtk2.prg

include $(Makefile_end_in)

