#CLIPINCLUDE = -I$(CLIPROOT)/include
Local_Depend = $(Clip_L_Dir)/libclip-ui.so $(Clip_L_Dir)/libclip-xml.so

.SUFFIXES: .prg .o .po
.PHONY: all clean

# Widget tests
PRG1=uitest$(EXESUFF)
PRG2=uiform$(EXESUFF)
LIBS=-lclip-ui -lclip-xml -lexpat
EXTRALIBS=

all: Makefile $(PRG1) $(PRG2)

cfg :

cleanc:
	rm -f$(V) $(PRG1) $(PRG2) $(OUTPUT) image[0-9]*

$(PRG1): uitest.prg $(CLIP) $(LIBCLIP)
	$(Trace_clip)
	$(CLIP) -o$@ -ewslM $(CLIPINCLUDE) uitest.prg $(LIBS) $(EXTRALIBS)

$(PRG2): uiform.prg $(CLIP) $(LIBCLIP)
	$(Trace_clip)
	$(CLIP) -o$@ -ewsln $(CLIPINCLUDE) uiform.prg $(LIBS) $(EXTRALIBS)

#.prg.o:
#	$(CLIP) -l $(CLIPINCLUDE) $<

include $(Makefile_o_in)

include $(Makefile_end_in)

