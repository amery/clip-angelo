Local_Depend =

.SUFFIXES: .prg .o .po
.PHONY: all clean

# Widget tests
PRG1=uitest$(EXESUFF)
PRG2=uiform$(EXESUFF)
LIBS=-lclip-ui -lclip-xml -lexpat
EXTRALIBS=

Depend_on = $(Clip_L_Dir)/libclip-ui.so $(Clip_L_Dir)/libclip-xml.so

################################################################################################################

all: Makefile $(PRG1) $(PRG2)

cfg :

cleanc:
	$(E_rm)rm -f$(V) $(PRG1) $(PRG2) image[0-9]*

distcleanc :
	$(E_rm)rm -rf$(V) *.g

$(PRG1): uitest.prg $(CLIP) $(LIBCLIP) $(Depend_on)
	$(Trace_clip)
	$(CLIP) -o$@ -ewslM $(CLIPINCLUDE) uitest.prg $(LIBS) $(EXTRALIBS)

$(PRG2): uiform.prg $(CLIP) $(LIBCLIP) $(Depend_on)
	$(Trace_clip)
	$(CLIP) -o$@ -ewsln $(CLIPINCLUDE) uiform.prg $(LIBS) $(EXTRALIBS)

include $(Makefile_o_in)

include $(Makefile_end_in)

