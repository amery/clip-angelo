#
#                                            clip-fcgi/example/Makefile.in
#
Depend_on 			= 				$(Clip_L_Dir)/libclip-fcgi.so
PRG 					= 				clip_echo.fcgi
LIBS 					= 				-lfcgi
export

######################################################################################
###################################################################################### local rules

all: Makefile $(PRG)$(EXESUFF)

install : all

hash1 :

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b $(PRG)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

$(PRG)$(EXESUFF): $(PRG).prg $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_rm)$(Trace_clip)
	$(CLIP) -o$@ -ewsl -M $(PRG).prg -lclip-fcgi $(LIBS)

include $(Makefile_o_in)

include $(Makefile_end_in)

