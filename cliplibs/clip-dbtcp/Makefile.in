#
#                                            clip-dbtcp/Makefile.in
#
#  GNU make makefile
#
Local_Depend =

PROG = clip-dbtcp
ADDLIBS=$(DBTCP_PATH)/libdbftp.a
DBTCPINCLUDE=-I$(DBTCP_PATH)/../include

C_FLAGS += $(DBTCPINCLUDE)
OBJS= $(sort dbtcp_clip.o dbtcp.o )

################################################################################################################

all: Makefile  $(STARGET)

cfg :

include $(Makefile_o_in)

install:all install-lib install-example

cleanc:
	$(E_rm)rm -rf$(V) dbtcp.c dbtcp_ref.c $(TARGET).*

distcleanc :
	$(E_rm)rm -rf$(V) *.g

include $(Makefile_end_in)

