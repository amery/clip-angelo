Local_Depend =
#$(Clip_L_Dir)/libclip-dbtcp.so

PRG=my_dbtcp$(EXESUFF)

################################################################################################################

all:Makefile  $(PRG)

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b $(PRG)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

my_dbtcp$(EXESUFF): my_dbtcp.prg $(CLIP) $(LIBCLIP)
	$(E_e)$(Trace_clip)
	$(CLIP) -p$@ -eMs my_dbtcp.prg -lclip-dbtcp

include $(Makefile_o_in)

include $(Makefile_end_in)

