Depend_on 		= 		$(Clip_L_Dir)/libclip-dbtcp.so
PRG 				= 		my_dbtcp$(EXESUFF)

export

######################################################################################
###################################################################################### local rules

all:Makefile  $(PRG)

install : all

hash1 :

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b $(PRG)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

my_dbtcp$(EXESUFF): my_dbtcp.prg $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_rm)$(Trace_clip)
	$(CLIP) -o$@ -eMwls my_dbtcp.prg -lclip-dbtcp

include $(Makefile_o_in)

include $(Makefile_end_in)

