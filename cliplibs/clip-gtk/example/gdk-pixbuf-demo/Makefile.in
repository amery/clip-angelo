#
#                                            clip-gtk/example/gdk-pixbuf-demo/Makefile.in
#

Local_Depend =
#$(Clip_L_Dir)/libclip-gtk.so

SRC=pixbuf_demo$(EXESUFF) pixbuf_scale$(EXESUFF) zoomimage$(EXESUFF)

all: Makefile $(SRC)

install : all

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c
	$(E_rm)rm -f$(V) $(SRC)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

pixbuf_demo$(EXESUFF) : pixbuf_demo.prg $(CLIP) $(LIBCLIP)
	$(Trace_clip)
	$(CLIP) -o$@ -ewsl pixbuf_demo.prg -lclip-gtk

pixbuf_scale$(EXESUFF) : pixbuf_scale.prg $(CLIP) $(LIBCLIP)
	$(Trace_clip)
	$(CLIP) -o$@ -ewsl pixbuf_scale.prg -lclip-gtk

zoomimage$(EXESUFF) : zoomimage.prg $(CLIP) $(LIBCLIP)
	$(Trace_clip)
	$(CLIP) -o$@ -ewsl zoomimage.prg -lclip-gtk

include $(Makefile_o_in)

include $(Makefile_end_in)

