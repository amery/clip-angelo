#
#                                            clip-gtk/example/Makefile.in
#
Depend_on 				= 			$(Clip_L_Dir)/libclip-gtk.so
DIRS_TO_SCAN 			= 			gdk-pixbuf-demo
SRC						=			$(sort 									\
													accel$(EXESUFF) 			\
													clipgtktest$(EXESUFF) 	\
													note$(EXESUFF) 			\
													simple$(EXESUFF) 			\
													test$(EXESUFF) 			\
													test_gtk$(EXESUFF) 		\
													test_image$(EXESUFF) 	\
													test_list$(EXESUFF) 		\
													window$(EXESUFF)  		\
													winpic$(EXESUFF) 			\
													ifactory$(EXESUFF) 		\
													clist_test$(EXESUFF) 	\
													t$(EXESUFF)					\
										)
#tbrow$(EXESUFF) 		# 		missing functions
LIBS 						+= 		$(PIXBUFLIBS)
export

######################################################################################
###################################################################################### local rules

all: Makefile $(SRC)

install : all $(DIRS_TO_SCAN)

hash1 :

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b
	$(E_rm)rm -f$(V) $(SRC)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

include $(Makefile_o_in)

clipgtktest$(EXESUFF): clipgtktest.prg $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@  $(CLIPFLAGS) -esw clipgtktest.prg -lclip-gtk $(LIBS)

accel$(EXESUFF): accel.prg $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@  $(CLIPFLAGS) -ews -M accel.prg -lclip-gtk $(LIBS)

