#
#                                            clip-gtk/example/Makefile.in
#
Depend_on 			= 			$(Clip_L_Dir)/libclip-gtk.so
DIRS_TO_SCAN 		= 			gdk-pixbuf-demo
prg_files			=			$(sort 									\
												accel.prg 			\
												clipgtktest.prg 	\
												note.prg 			\
												simple.prg 			\
												test.prg 			\
												test_gtk.prg 		\
												test_image.prg 	\
												test_list.prg 		\
												window.prg  		\
												winpic.prg 			\
												ifactory.prg 		\
												clist_test.prg 	\
												t.prg					\
										)
#tbrow.prg 		# 		missing functions
exe_files 			= 			$(addsuffix $(EXESUFF), $(basename $(prg_files) ) )
LIBS 					+= 		$(PIXBUFLIBS)
SRC_C_files 		= 			$(sort $(c_files) )
SRC_PRG_files 		= 			$(sort $(prg_files) )
export

######################################################################################
###################################################################################### local rules

all: Makefile $(exe_files)

install : all $(DIRS_TO_SCAN)

hash1 :

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b
	$(E_rm)rm -f$(V) $(exe_files)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

include $(Makefile_o_in)

clipgtktest$(EXESUFF): clipgtktest.prg $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@  $(CLIPFLAGS) -esw clipgtktest.prg -lclip-gtk $(LIBS)

accel$(EXESUFF): accel.prg $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@  $(CLIPFLAGS) -ews -M accel.prg -lclip-gtk $(LIBS)

