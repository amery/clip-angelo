#
#                                            clip-gtk2/example/gdk-pixbuf-demo/Makefile.in
#

Local_Depend =
#$(Clip_L_Dir)/libclip-gtk2.so

SRC=pixbuf_demo$(EXESUFF) pixbuf_scale$(EXESUFF) zoomimage$(EXESUFF)

################################################################################################################

all: Makefile $(SRC)

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c
	$(E_rm)rm -f$(V) $(SRC)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

pixbuf_demo$(EXESUFF) : pixbuf_demo.prg $(CLIP) $(LIBCLIP)
	$(Trace_clip)
	$(CLIP) -o$@ -ews pixbuf_demo.prg -lclip-gtk2

pixbuf_scale$(EXESUFF) : pixbuf_scale.prg $(CLIP) $(LIBCLIP)
	$(Trace_clip)
	$(CLIP) -o$@ -ews pixbuf_scale.prg -lclip-gtk2

zoomimage$(EXESUFF) : zoomimage.prg $(CLIP) $(LIBCLIP)
	$(Trace_clip)
	$(CLIP) -o$@ -ews zoomimage.prg -lclip-gtk2

include $(Makefile_o_in)

include $(Makefile_end_in)

