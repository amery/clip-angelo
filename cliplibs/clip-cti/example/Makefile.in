#
#                                            clip-cti/example/Makefile.in
#
Local_Depend =
#$(Clip_L_Dir)/libclip-cti.so
Local_Depend +=
# $(Clip_L_Dir)/libclip-fw.so

CLIPFLAGS += -n
OBJS  =
ADD_OBJS =
POBJS  =

.SUFFIXES: .prg .o .po

################################################################################################################

all:Makefile  ctitest$(EXESUFF) ctifd$(EXESUFF) browsecombo$(EXESUFF)

cfg :

cleanc:
	$(E_rm)rm -rf$(V) *.c *.b
	$(E_rm)rm -f$(V) ctitest$(EXESUFF) ctifd$(EXESUFF) browsecombo$(EXESUFF)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

include $(Makefile_o_in)

ctitest$(EXESUFF): ctitest.prg $(OBJS) $(CLIP) $(LIBCLIP)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -swe ctitest.prg $(OBJS) $(ADD_OBJS) $(CLIPLIBS)

ctifd$(EXESUFF): ctifd.prg $(OBJS) $(CLIP) $(LIBCLIP)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -swe -M  ctifd.prg $(OBJS) $(ADD_OBJS) $(CLIPLIBS)

ctit$(EXESUFF): ctit.prg $(OBJS) $(CLIP) $(LIBCLIP)
	$(E_e)$(Trace_clip)
#	$(CLIP) -swe -M  ctit.prg $(OBJS) $(ADD_OBJS) $(CLIPLIBS)
	$(CLIP) -o$@ -swe  ctit.prg $(OBJS) $(ADD_OBJS) $(CLIPLIBS) -lclip-fw

browsecombo$(EXESUFF): browsecombo.prg $(OBJS) $(CLIP)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -swe browsecombo.prg $(OBJS) $(ADD_OBJS) $(CLIPLIBS)

depend:
	$(E_xx)makedepend -w80 -- *.prg 2>/dev/null

include $(Makefile_end_in)

