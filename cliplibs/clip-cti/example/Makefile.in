#
#                                            clip-cti/example/Makefile.in
#
Depend_on 				= 			$(Clip_L_Dir)/libclip-cti.so
# 		\
#										$(Clip_L_Dir)/libclip-fw.so
INCDIR 					= 			-I$(Clip_M_Dir)/cliplibs/clip-cti/include
LIBS 						= 			-lclip-cti
ADD_OBJS 				=
BINS 						= 			$(sort 								\
												ctitest$(EXESUFF) 		\
												ctifd$(EXESUFF) 			\
												browsecombo$(EXESUFF) 	\
										)
#												ctit$(EXESUFF) 			\
export

######################################################################################
###################################################################################### local rules

all : Makefile

install : all

hash1 :

cfg :

cleanc:
	$(E_rm)rm -rf$(V) *.c *.b
	$(E_rm)rm -f$(V) ctitest$(EXESUFF) ctifd$(EXESUFF) browsecombo$(EXESUFF) ctit$(EXESUFF)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

include $(Makefile_o_in)

ctitest$(EXESUFF): ctitest.prg $(OBJS) $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -swe $(INCDIR) ctitest.prg $(OBJS) $(ADD_OBJS) $(LIBS)

ctifd$(EXESUFF): ctifd.prg $(OBJS) $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -swe $(INCDIR) -M  ctifd.prg $(OBJS) $(ADD_OBJS) $(LIBS)

ctit$(EXESUFF): ctit.prg $(OBJS) $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
#	$(CLIP) -swe -M  ctit.prg $(OBJS) $(ADD_OBJS)
	$(CLIP) -o$@ $(INCDIR) -swe  ctit.prg $(OBJS) $(ADD_OBJS) -lclip-fw $(LIBS)

browsecombo$(EXESUFF): browsecombo.prg $(OBJS) $(CLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ $(INCDIR) -swe browsecombo.prg $(OBJS) $(ADD_OBJS) $(LIBS)

depend:
	makedepend -w80 -- *.prg 2>/dev/null

include $(Makefile_end_in)

