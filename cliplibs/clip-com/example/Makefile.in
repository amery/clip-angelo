Local_Depend = $(Clip_L_Dir)/libclip-com.so

PRG=com

all: Makefile $(PRG)(EXESUFF)

cfg :

cleanc:
	rm -f$(V) *.c *.b $(PRG) *.bz2

$(PRG)(EXESUFF): $(PRG).prg $(CLIP) $(LIBCLIP)
	$(Trace_clip)
	$(CLIP) -o$@ -e -M $(PRG).prg -lclip-$(PRG)

include $(Makefile_o_in)

include $(Makefile_end_in)

