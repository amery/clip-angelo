Depend_on 		= 			$(Clip_L_Dir)/libclip-com.so
PRG 				= 			com

export

######################################################################################
###################################################################################### local rules

all: Makefile $(PRG)$(EXESUFF)

install : all

hash1 :

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b $(PRG) *.bz2

distcleanc :
	$(E_rm)rm -rf$(V) *.g

$(PRG)$(EXESUFF): $(PRG).prg $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -ewNsl -M $(PRG).prg -lclip-$(PRG)

include $(Makefile_o_in)

include $(Makefile_end_in)

