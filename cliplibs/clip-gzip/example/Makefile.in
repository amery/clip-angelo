Depend_on 			= 				$(Clip_L_Dir)/libclip-gzip.so
PRG 					= 				gz$(EXESUFF)
LIBS 					= 				-lclip-gzip -lz
export

######################################################################################
###################################################################################### local rules

all : Makefile  $(PRG)

install : all

hash1 :

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b $(PRG) *.gz

distcleanc :
	$(E_rm)rm -rf$(V) *.g

gz$(EXESUFF): gz.prg $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -ew -s -l -M gz.prg $(LIBS)

include $(Makefile_o_in)

include $(Makefile_end_in)

