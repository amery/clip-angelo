Local_Depend =
# $(Clip_L_Dir)/libclip-gzip.so

PRG=gz$(EXESUFF)

################################################################################################################

all:Makefile  $(PRG)

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b $(PRG) *.gz

distcleanc :
	$(E_rm)rm -rf$(V) *.g

gz$(EXESUFF): gz.prg $(CLIP) $(LIBCLIP)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -e -s -l -M gz.prg -lclip-gzip

include $(Makefile_o_in)

include $(Makefile_end_in)

