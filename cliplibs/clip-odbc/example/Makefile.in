Depend_on 			= 			$(Clip_L_Dir)/libclip-odbc.so 	\
									$(Clip_L_Dir)/libclip-oob.so
PRG 					=			my_isql

export

######################################################################################
###################################################################################### local rules

all: Makefile $(PRG)$(EXESUFF)

install : all

hash1 :

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b $(PRG)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

$(PRG)$(EXESUFF): $(PRG).prg $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) $(CLIPFLAGS) -o$@ -ewMs $(PRG).prg -lclip-odbc -L$(LIBODBC) -lodbc

include $(Makefile_o_in)

include $(Makefile_end_in)

