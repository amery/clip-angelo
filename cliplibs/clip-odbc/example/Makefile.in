Local_Depend = $(Clip_L_Dir)/libclip-odbc.so $(Clip_L_Dir)/libclip-oob.so


PRG=my_isql

all: Makefile $(PRG)

cfg :

cleanc:
	rm -f$(V) *.c *.b $(PRG)

$(PRG)$(EXESUFF): $(PRG).prg $(CLIP) $(LIBCLIP)
