#
#                                            clip-crypto/example/Makefile.in
#
Depend_on 			= 				$(Clip_L_Dir)/libclip-crypto.so
PRG 					= 				crypto$(EXESUFF)
LIBS 					= 				-lcrypto
export

######################################################################################
###################################################################################### local rules

all: Makefile $(PRG)

install : all

hash1 :

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b $(PRG)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

crypto$(EXESUFF): crypto.prg $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -ewN -s -l -M crypto.prg -lclip-crypto $(LIBS)

include $(Makefile_o_in)

include $(Makefile_end_in)

