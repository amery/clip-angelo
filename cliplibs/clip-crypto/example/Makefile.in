Local_Depend =
# $(Clip_L_Dir)/libclip-crypto.so

PRG=crypto$(EXESUFF)

################################################################################################################

all: Makefile $(PRG)

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b $(PRG)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

crypto$(EXESUFF): crypto.prg $(CLIP) $(LIBCLIP)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -e -s -l -M crypto.prg -lclip-crypto

include $(Makefile_o_in)

include $(Makefile_end_in)

