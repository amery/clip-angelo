#
#                                            clip-netto/Makefile.in
#

Local_Depend 			= 			include/*.ch
PROG 						= 			clip-netto
SRCS 						= 			$(sort 						\
													fnbin2i.prg 	\
													fnbit.prg  		\
													fndpve.prg  	\
													fnerr.prg  		\
													fnhex.prg  		\
													fnpeek.prg  	\
													fnpfeval.prg  	\
													ipxinit.prg  	\
													is3x.prg  		\
													isnet.prg  		\
													namel.prg  		\
													nonull.prg  	\
													nwdate.prg  	\
													reg2l.prg  		\
													request.prg  	\
													ssfp.prg			\
										)
OBJS  					= 			$(addsuffix .o, $(basename $(SRCS) ) )
# fnbit.o fnpfeval.o is3x.o isnet.o ipxinit.o request.o fnpeek.o ## error missing functions
DOC 						= 			doc/*
ADDLIBS 					=

export

######################################################################################
###################################################################################### local rules

all : Makefile $(OBJS) $(STARGET)

hash1 :
	$(E_xx)$(Clip_M_Dir)/init/LinkAllFiles.sh clip-netto

cfg :
	$(E_mkD)mkdir -p$(V) $(Clip_I_Dir)/$(PROG)
	$(E_cp)cp --remove-destination -fpu$(V) include/*.ch include/*.h $(Clip_I_Dir)/$(PROG)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

cleanc :
	$(E_rm)rm -f$(V) $(TARGET).* *.c

install : all install-lib install-doc

include $(Makefile_o_in)

include $(Makefile_end_in)

