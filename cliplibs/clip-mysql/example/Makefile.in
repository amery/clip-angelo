#
#                                            clip-mysql/example/Makefile.in
#
Depend_on 			= 				$(Clip_L_Dir)/libclip-mysql.so
PRG 					= 				my_mysql$(EXESUFF)
LIBS 					= 				-L/usr/lib/mysql -lmysqlclient
export

######################################################################################
###################################################################################### local rules

all: Makefile $(PRG)

install : all

hash1 :

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b $(PRG)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

my_mysql$(EXESUFF): my_mysql.prg $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -ewMs my_mysql.prg -lclip-mysql $(LIBS)

include $(Makefile_o_in)

include $(Makefile_end_in)

