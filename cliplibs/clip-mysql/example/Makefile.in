#
Local_Depend =
# $(Clip_L_Dir)/libclip-mysql.so

PRG=my_mysql$(EXESUFF)

all: Makefile $(PRG)

install : all

cfg :

cleanc:
	$(E_rm)rm -f$(V) *.c *.b $(PRG)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

my_mysql$(EXESUFF): my_mysql.prg $(CLIP) $(LIBCLIP)
	$(Trace_clip)
	$(CLIP) -o$@ -ewMs my_mysql.prg -lclip-mysql

include $(Makefile_o_in)

include $(Makefile_end_in)

