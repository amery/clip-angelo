#INCLUDEDIRS=-I. -I$(CLIPROOT)/include
Local_Depend =
#$(Clip_L_Dir)/libclip-rtf.so

#CLIPFLAGS = -l -n -w
CLIPLIBS  =
BINS = rtfdemo$(EXESUFF)

.SUFFIXES: .prg .o .po

all: Makefile $(BINS)

cfg :

rtfdemo$(EXESUFF) : rtfdemo.prg $(CLIP) $(LIBCLIP)
	$(Trace_clip)
	$(CLIP) -o$@-e -s -l -M rtfdemo.prg -lclip-rtf


cleanc:
	rm -f$(V) *.c *.rtf $(BINS)

distcleanc :
	rm -rf$(V) *.g

include $(Makefile_o_in)

include $(Makefile_end_in)

