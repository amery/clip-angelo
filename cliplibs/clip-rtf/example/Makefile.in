Local_Depend =

CLIPLIBS  =
BINS = rtfdemo$(EXESUFF)

.SUFFIXES: .prg .o .po

################################################################################################################

all: Makefile $(BINS)

cfg :

rtfdemo$(EXESUFF) : rtfdemo.prg $(CLIP) $(LIBCLIP)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@-e -s -l -M rtfdemo.prg -lclip-rtf

cleanc:
	$(E_rm)rm -f$(V) *.c *.rtf $(BINS)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

include $(Makefile_o_in)

include $(Makefile_end_in)

