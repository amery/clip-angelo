Depend_on 				=				$(Clip_L_Dir)/libclip-rtf.so
BINS 						= 				rtfdemo$(EXESUFF)

export

######################################################################################
###################################################################################### local rules

all: Makefile $(BINS)

install : all

hash1 :

cfg :

rtfdemo$(EXESUFF) : rtfdemo.prg $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_e)$(Trace_clip)
	$(CLIP) -o$@ -ewslM rtfdemo.prg -lclip-rtf


cleanc:
	$(E_rm)rm -f$(V) *.c *.rtf $(BINS)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

include $(Makefile_o_in)

include $(Makefile_end_in)

