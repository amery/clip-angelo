Procedure cn_DefaultInit ()
	if Empty ( p_aUseStdCh ) ;	aAdd ( p_aUseStdCh, "cn_std.ch") ; 	EndIf
	If Empty ( p_sOutDir ) ; 	p_sOutDir := "./"   	; 	EndIf
