	If Empty ( p_sTargetCharset ) 	; 	p_sTargetCharset := ""   ; 	EndIf
	If Empty ( veCC )   	; 	veCC := "gcc"    	; 	EndIf
	If Empty ( veCDBG )   ; 	veCDBG := "-g"    ; 	EndIf
/*	If Empty ( veCFLAGS )   ; 	veCFLAGS := ""    ; 	EndIf */
