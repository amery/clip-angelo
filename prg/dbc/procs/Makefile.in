Local_Depend = ../dbc/deflist.ch

CLIPFLAGS += -I../dbc
CLIPLIBS  =
OBJS  =  $(sort 	procs1.o procs3.o procs5.o procs7.o 	\
	 					procs8.o procini.o procscri.o 			\
	 					procared.o procsay.o )

.SUFFIXES: .prg .o

################################################################################################################

install all:    $(OBJS) ../procs.a

../procs.a : $(OBJS)
	$(E_c_mkl)$(CLIP_MAKELIB) ../procs.a *.o

cfg :

cleanc:
	$(E_rm)rm -rf$(V) *.c *.b

distcleanc :
	$(E_rm)rm -rf$(V) *.g

include $(Makefile_o_in)

include $(Makefile_end_in)

