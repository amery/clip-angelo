#
#                                            prg/codb/Makefile.in
#
Depend_on 			=			$(Clip_L_Dir)/libclip-codb.so 	\
									$(Clip_L_Dir)/libcodb-query.so
LIBS  				= 			-lclip-codb -lcodb-query
TARGET 				= 			codb$(EXESUFF)
OBJS  =

export

######################################################################################
###################################################################################### local rules

all : Makefile $(OBJS) $(TARGET)

hash1 :

cfg :

cleanc :
	$(E_rm)rm -f$(V) *.c *.b
	$(E_rm)rm -f$(V) $(TARGET)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

$(TARGET): codb.prg $(OBJS) Makefile $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_clip)if [ -f $(subst .prg,.g,$<) ] ; then 						\
		$(Trace_clip_d) 	;														\
		$(CLIP) $(CLIPFLAGS) -g -o $@ -eslNn $< $(OBJS) $(LIBS) ;	\
	else 																				\
		$(Trace_clip) 		;														\
		$(CLIP) $(CLIPFLAGS) -o $@ -eslNn $< $(OBJS) $(LIBS) ;		\
	fi

install : all
	$(E_cp)cp --remove-destination -fpu$(V) $(TARGET) $(Clip_B_Dir)/

include $(Makefile_o_in)

include $(Makefile_end_in)

