Local_Depend =

.SUFFIXES: .prg .o .po

MODULES = 	$(sort 	acc_chart.po acc_oper.po attr.po class.po  	\
							counter.po default.po deposit.po extent.po 	\
							group.po index.po plugins.po report.po 		\
							tcolumn.po tview.po user.po )

################################################################################################################

install all : Makefile $(DIRS_TO_SCAN) $(MODULES)

cfg :

cleanc : $(DIRS_TO_SCAN)
	$(E_rm)rm -rf$(V) *.c *.b $(MODULES)
	$(E_rm)rm -rf$(V) *.pro $(PROGS) test test2

distcleanc :
	$(E_rm)rm -rf$(V) *.g

include $(Makefile_o_in)

include $(Makefile_end_in)

