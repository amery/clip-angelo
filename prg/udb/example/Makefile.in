#
#                                            prg/udb/example/Makefile.in
#
Local_Depend =

CLIPLIBS  = -lclip-codb
PROGS = my001$(EXESUFF) cgi_currency$(EXESUFF)

.SUFFIXES: .prg .o .po

################################################################################################################

all: Makefile $(PROGS)

cfg :

cleanc:
	$(E_rm)rm -rf$(V) *.c *.b
	$(E_rm)rm -rf$(V) $(PROGS)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

include $(Makefile_o_in)

my001$(EXESUFF):  my001.prg $(CLIP) $(LIBCLIP)
	$(Trace_clip)
	$(CLIP) $(CLIPFLAGS) -o$@ -es -M my001.prg $(CLIPLIBS)

cgi_currency$(EXESUFF): cgi_currency.prg $(CLIP) $(LIBCLIP)
	$(Trace_clip)
	$(CLIP) $(CLIPFLAGS) -o$@ -e -M cgi_currency.prg $(CLIPLIBS)

include $(Makefile_end_in)

