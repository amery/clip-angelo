#
#                                            prg/udb/example/Makefile.in
#
Local_Depend 			=
LIBS  					= 				-lclip-codb
PROGS 					= 				$(sort 									\
														my001$(EXESUFF) 			\
														cgi_currency$(EXESUFF) 	\
											)

export

######################################################################################
###################################################################################### local rules

install all: Makefile $(PROGS)

hash1 :

cfg :

cleanc:
	$(E_rm)rm -rf$(V) *.c *.b
	$(E_rm)rm -rf$(V) $(PROGS)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

include $(Makefile_o_in)

my001$(EXESUFF):  my001.prg $(CLIP) $(LIBCLIP)
	$(E_e)$(Trace_clip)
	$(CLIP) $(CLIPFLAGS) -o$@ -eswn my001.prg $(LIBS)

cgi_currency$(EXESUFF): cgi_currency.prg $(CLIP) $(LIBCLIP)
	$(E_e)$(Trace_clip)
	$(CLIP) $(CLIPFLAGS) -o$@ -ew -M cgi_currency.prg $(LIBS)

include $(Makefile_end_in)

