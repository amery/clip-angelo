Local_Depend =

LIBS = -lwrap
#CFLAGS = -O

all install : Makefile tcp-wrap$(EXESUFF)

cfg :

tcp-wrap$(EXESUFF): tcp-wrap.c Makefile
	$(E_cc)if [ -f $(subst .c,.g,$<) ] ; then 	\
		$(Trace_c_d) ; 									\
		$(CC) -g $(C_FLAGS) $< -o $@ $(LIBS); 	\
	else 														\
		$(Trace_c) ; 										\
		$(CC) $(C_FLAGS) $< -o $@ $(LIBS); 		\
	fi

cleanc:
	$(E_rm)rm -f$(V) tcp-wrap$(EXESUFF)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

include $(Makefile_o_in)

include $(Makefile_end_in)

