#
#                                            prg/cobra_clnt/Makefile.in
#
Depend_on 			= 		$(Clip_L_Dir)/libclip-codb.so
CLIPFLAGS 			+= 	-n
OBJS  				= 		objbrowse.o tviewbrowse.o

export

######################################################################################
###################################################################################### local rules

all: Makefile $(OBJS) cobra_clnt1$(EXESUFF)

hash1 :

cfg :

cleanc:
	$(E_rm)rm -rf$(V) *.c *.b
	$(E_rm)rm -rf$(V) cobra_clnt1$(EXESUFF)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

cobra_clnt1$(EXESUFF) : cobra_clnt1.prg $(OBJS) Makefile $(CLIP) $(LIBCLIP) $(Depend_on)
	$(E_clip)if [ -f $(subst .prg,.g,$<) ] ; then 		\
		$(Trace_clip_d) 	;										\
		$(CLIP) $(CLIPFLAGS) -g -o $@ -es $< $(OBJS) ;	\
	else 																\
		$(Trace_clip) 		;										\
		$(CLIP) $(CLIPFLAGS) -o $@ -es $< $(OBJS) ;		\
	fi

install : all
	$(E_cp)cp --remove-destination -fu$(V) cobra_clnt1$(EXESUFF) $(Clip_B_Dir)/
	$(E_mkD)mkdir -p$(V) $(INSTDIR)/cobra
	$(E_cp)cp --remove-destination -fu$(V) cobra_clnt.ini $(INSTDIR)/cobra

include $(Makefile_o_in)

include $(Makefile_end_in)

