#
#                                            prg/pp/Makefile.in
#
Local_Depend 	=
Depend_on 		=
LIBS  			=
OBJS 				=
BIN 				= 		pp_ron$(EXESUFF)

export

######################################################################################
###################################################################################### local rules

$(BIN):	pp.prg Makefile $(CLIP) $(LIBCLIP)
	$(E_clip)if [ -f $(subst .prg,.g,$<) ] ; then 					\
		$(Trace_clip_d) 	;													\
		$(CLIP) $(CLIPFLAGS) -g -o $@ -es $< $(OBJS) $(LIBS) ;	\
	else 																			\
		$(Trace_clip) ;														\
		$(CLIP) $(CLIPFLAGS) -o $@ -es $< $(OBJS) $(LIBS) ;		\
	fi

hash1 :

cfg :

 all : Makefile  $(BIN)

install : all
	$(E_cp)cp --remove-destination -fpu$(V) $(BIN) $(INSTDIR)/bin/
		$(E_cp)cp --remove-destination -fpu$(V) *.ch $(INSTDIR)/include

cleanc:
	$(E_rm)rm -f$(V) *.c *.ntx $(BIN) *.pp?

distcleanc :
	$(E_rm)rm -rf$(V) *.g

include $(Makefile_o_in)

include $(Makefile_end_in)

