MODULES 					= 			$(sort 							\
													kmch_hello.po 		\
													mk_tmpdir.po		\
										)
#kmch_codb.po)

export

######################################################################################
###################################################################################### local rules

all : Makefile $(MODULES)

hash1 :

cfg :

cleanc :
	$(E_rm)rm -rf$(V) *.c *.b $(MODULES)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

install : all
	$(E_mkD)mkdir -p$(V) $(INSTDIR)/kamache/mod-ini
	$(E_cp)cp --remove-destination -fpu$(V) Makefile *.po *.prg $(INSTDIR)/kamache/mod-ini

include $(Makefile_o_in)

include $(Makefile_end_in)

