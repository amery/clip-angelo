Local_Depend 			=
MODULES 					= 				hello.po

export

######################################################################################
###################################################################################### local rules

all : $(MODULES)

hash1 :

cfg :

cleanc:
	$(E_rm)rm -rf$(V) *.c *.b $(MODULES)

distcleanc :
	$(E_rm)rm -rf$(V) *.g

install : all
	$(E_mkD)mkdir -p$(V)  $(INSTDIR)/kamache/mod-clip
	$(E_cp)cp --remove-destination -fpu$(V) Makefile *.po *.prg $(INSTDIR)/kamache/mod-clip

include $(Makefile_o_in)

include $(Makefile_end_in)

