#
#                                            prg/hiper/Makefile.in
#
Local_Depend =


#CLIP	= $(CLIPROOT)/bin/clip
#CLIPFLAGS = -s -l -w

BINS = $(sort clip_hindex$(EXESUFF) clip_hseek$(EXESUFF) )
OBJS =
LIBS =

all : Makefile $(BINS)

cfg :

install :	all
	cp --remove-destination -fpu$(V) clip_hindex$(EXESUFF) $(DESTDIR)/bin/clip_hindex$(EXESUFF)
	cp --remove-destination -fpu$(V) clip_hseek$(EXESUFF) $(DESTDIR)/bin/clip_hseek$(EXESUFF)

cleanc:
	rm -rf$(V) $(BINS) *.c *.b

distcleanc :
	rm -rf$(V) *.g

include $(Makefile_o_in)

clip_hindex$(EXESUFF):	clip_hindex.prg Makefile $(CLIP) $(LIBCLIP)
	$(E_if)if [ -f $(subst .prg,.g,$<) ] ; then 			\
		$(Trace_clip_d) 	;										\
		$(CLIP) $(CLIPFLAGS) -g -o$@ -e $< ; 				\
	else 																\
		$(Trace_clip) 		;										\
		$(CLIP) $(CLIPFLAGS) -o$@ -e $< ;					\
	fi

clip_hseek$(EXESUFF):	clip_hseek.prg Makefile $(CLIP) $(LIBCLIP)
	$(E_if)if [ -f $(subst .prg,.g,$<) ] ; then 			\
		$(Trace_clip_d) 	;										\
		$(CLIP) $(CLIPFLAGS) -g -o$@ -e $< ; 				\
	else 																\
		$(Trace_clip) 		;										\
		$(CLIP) $(CLIPFLAGS) -o$@ -e $< ;					\
	fi

include $(Makefile_end_in)

