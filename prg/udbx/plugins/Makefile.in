#
#                                            prg/udbx/plugins/Makefile.in
#

Local_Depend =
.SUFFIXES: .prg .o .po

MODULES = 	$(sort 	attr.po class.po counter.po default.po deposit.po 							\
							group.po index.po plugins.po report.po tcolumn.po tview.po user.po )

CLIPFLAGS += -I ../../../cliplibs/clip-gtk2/include

################################################################################################################

all : Makefile $(DIRS_TO_SCAN) $(MODULES)

cfg :

commit:
	../_cvs commit
update:
	../_cvs update -dP

cleanc : $(DIRS_TO_SCAN)
	$(E_rm)rm -rf$(V) *.c *.b $(MODULES)
	$(E_rm)rm -rf$(V) *.pro test test2

distcleanc :
	$(E_rm)rm -rf$(V) *.g

install : all

include $(Makefile_o_in)

include $(Makefile_end_in)

